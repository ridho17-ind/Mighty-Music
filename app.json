{
  "name": "Mighty Music",
  "description": "Deploy bot VC Group kamu untuk memutar lagu di Obrolan Suara Grup Telegram Anda. Didukung oleh PyTgCalls.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/Yunus-ZEND/Mighty-Music",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Session string dalam bentuk Pyogram",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Ambil Bot Token Mu dari @botfather",
      "required": true
    },
    "BOT_NAME": {
      "description": "Username Bot yg kamu buat.",
      "required": false,
      "value": ""
    },
    "API_ID": {
      "description": "App ID dari my.telegram.org/apps",
      "required": true
    },
    "API_HASH": {
      "description": "App hash dari my.telegram.org/apps",
      "required": true
    },
    "SUDO_USERS": {
      "description": "Letakkan ID mu disini sebagai pemilik Bot Musik ini.",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Batas durasi audio maksimal untuk download (menit).",
      "required": true,
      "value": "10"
    }
  }
}

